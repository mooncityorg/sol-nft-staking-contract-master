  "accounts": [
    {
      "name": "GlobalPool",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "superAdmin",
            "type": "publicKey"
          },
          {
            "name": "totalStakedCount",
            "type": "u64"
          }
        ]
      }
    },
    {
      "name": "UserPool",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "owner",
            "type": "publicKey"
          },
          {
            "name": "stakedCount",
            "type": "u64"
          },
          {
            "name": "stakedMints",
            "type": {
              "array": [
                {
                  "defined": "StakedData"
                },
                100
              ]
            }
          },
          {
            "name": "lastRewardTime",
            "type": "i64"
          },
          {
            "name": "remainingRewards",
            "type": "u64"
          }
        ]
      }
    }
  ],
